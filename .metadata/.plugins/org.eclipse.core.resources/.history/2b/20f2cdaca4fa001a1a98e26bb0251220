package Searching;

public class squareroot {
	int sqrroot(int x) {
		int low = 1, high = x, ans = -1;
		while(low < high) {
			int mid = (high + low)/2;
			int midsqr = mid * mid;
			if (x ==  midsqr){
				return mid;
			}
			else if (midsqr > x) {
				high = mid -1;
			}
			else(midsqr < x){
				low = mid + 1;
				ans = mid;
			}
		}
		return -1;
	}
}
