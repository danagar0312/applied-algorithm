package Sorting;

public class SortElementsTwoTypes {
	
	void sort(int[] arr, int n) {
		int[] temp = new int[arr.length];
		int i;
		
		for(i = 0 ; i < arr.length ; i++) {
			if (arr[i] < 0) {
				temp[i] = arr[i];
			}
		}
		
		for (int j = 0; j < n; j++) {
			if (arr[j] > 0) {
				temp[i] = arr[j];
				i++;
			}
		}
	}
}
