package Arrays;

import java.util.Arrays;

public class smallerNumbersThanCurrent {
	public int[] smallerNumberCount(int[] nums){
		int[] smaller = new int[101];
		for(int i = 0 ; i < nums.length ; i++) {
			smaller[nums[i]]++;
		}
		
		System.out.println(Arrays.print(smaller));
		
		
		return nums;
		
	}
}
