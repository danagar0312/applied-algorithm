package Arrays;

public class shufflearray {
	 public int[] shuffle(int[] nums, int n) {
	       
	        int len = nums.length;
	        int traverse = len/2 - 1;
	        int[] result = new int[len];
	        for (int i = 0; i <= traverse ; i++){
	            result[2*i] = nums[i];
	            result[2*i+1] = nums[i+n];
	        }
	        return result;
	    }
}
